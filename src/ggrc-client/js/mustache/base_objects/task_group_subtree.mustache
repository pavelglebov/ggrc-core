  <li data-object-id="{{instance.id}}" data-object-type="{{instance.class.table_singular}}">
    <div class="item-main objective-selector" data-model="true" {{#instance}}{{data "model"}}{{/instance}}>
      <span class="inline-data-content oneline" rel="tooltip" title="{{instance.title}}">
        <i class="fa fa-{{instance.class.table_singular}} color"></i>
        {{instance.title}}
        {{^if_equals parent_instance.workflow.reify.status 'Inactive'}}
            {{#is_allowed 'update' parent_instance context='for'}}
                <a href="javascript://" data-toggle="unmap" data-id="{{instance.id}}"
                   data-type="{{instance.class.shortName}}" class="deleteTrigger">
                    <i class="fa fa-trash"></i>
                    <span class="result" {{data 'result'}}></span>
                </a>
            {{/is_allowed}}
        {{/if_equals}}
      </span>
    </div>
  </li>
