{{#if isDisabled}}
    {{#if isSelected}}
      <input type="checkbox" checked="checked" disabled/>
    {{else}}
      <input type="checkbox" disabled/>
    {{/if}}
{{else}}
  {{#if isSelected}}
  <input type="checkbox" checked="checked"/>
  {{else}}
    <input type="checkbox"/>
  {{/if}}
{{/if}}
